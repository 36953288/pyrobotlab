<li value="fast"><think><set name="sfi1-1">1</set><set name="sfi1-2">1</set><set name="sfi1-3">1</set><set name="sfi1-4">1</set><set name="sfi1-5">1</set><set name="sfi1-6">1</set></think></li>
</condition><think><set name="SRHS1">ok</set></think>
</template>
