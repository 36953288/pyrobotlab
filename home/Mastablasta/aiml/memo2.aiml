<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">

<category><pattern>DO YOU KNOW #</pattern>
<template><random>
<li>Yes, I know <star/></li>
<li>Sure I know <star/></li>
<li>Of course I know <star/></li></random>
<think><set name="subject"><star/></set></think>
<oob><mrl><service>python</service><method>exec</method><param>
COUNTER()
</param></mrl></oob>
</template>
</category>

<category><pattern>SUBJECT1</pattern>
<template>
<think>
<set name="timeset1"><date format="%B %d" jformat="MMMMMMMMM dd"/></set>
<set name="subject1"><get name="subject"/></set>
<set name="subnumber1">1</set>
</think>
</template>
</category>

<category><pattern>SUBJECT2</pattern>
<template>
<think>
<condition name="subject">
<li><value><get name="subject1"/></value><set name="subject"><get name="subject1"/></set><set name="subnumber1">2</set><think><set name="ANSWER">ok</set></think></li>
<li><set name="timeset2"><date format="%B %d" jformat="MMMMMMMMM dd"/></set>
<set name="subject2"><get name="subject"/></set><set name="subnumber2">1</set></li></condition>
</think>
</template>
</category>

<category><pattern>SUBJECT3</pattern>
<template>
<think>
<condition name="subject">
<li><value><get name="subject1"/></value><set name="subject"><get name="subject1"/></set><set name="subnumber1">2</set><think><set name="ANSWER">ok</set></think></li>
<li><value><get name="subject2"/></value><set name="subject"><get name="subject2"/></set><set name="subnumber2">2</set><think><set name="ANSWER">ok</set></think></li>
<li><set name="timeset3"><date format="%B %d" jformat="MMMMMMMMM dd"/></set>
<set name="subject3"><get name="subject"/></set><set name="subnumber3">1</set></li></condition>
</think>
</template>
</category>

<category><pattern>SUBJECT4</pattern>
<template>
<think>
<condition name="subject">
<li><value><get name="subject1"/></value><set name="subject"><get name="subject1"/></set><set name="subnumber1">2</set><think><set name="ANSWER">ok</set></think></li>
<li><value><get name="subject2"/></value><set name="subject"><get name="subject2"/></set><set name="subnumber2">2</set><think><set name="ANSWER">ok</set></think></li>
<li><value><get name="subject3"/></value><set name="subject"><get name="subject3"/></set><set name="subnumber3">2</set><think><set name="ANSWER">ok</set></think></li>
<li><set name="timeset4"><date format="%B %d" jformat="MMMMMMMMM dd"/></set>
<set name="subject4"><get name="subject"/></set><set name="subnumber4">1</set></li></condition>
</think>
</template>
</category>

<category><pattern># YESTERDAY</pattern>
<template><think>
<set name="timeget1"><date format="%B %d" jformat="MMMMMMMMM dd"/></set>
<set name="timeget2"><date format="%B %d" jformat="MMMMMMMMM dd"/></set>
<set name="timeget3"><date format="%B %d" jformat="MMMMMMMMM dd"/></set>
<set name="timeget4"><date format="%B %d" jformat="MMMMMMMMM dd"/></set>
<learn><category>
<pattern>MEMOTRIGGER</pattern>
<template><think>
    <set name="time1">
        <interval><jformat>MMMMMMMMM dd</jformat>
        <style>days</style>
        <from><get name="timeset1"/></from>
        <to><get name="timeget1"/></to>
        </interval></set>
    <set name="time2">
        <interval><jformat>MMMMMMMMM dd</jformat>
        <style>days</style>
        <from><get name="timeset2"/></from>
        <to><get name="timeget2"/></to>
        </interval></set>
    <set name="time3">
        <interval><jformat>MMMMMMMMM dd</jformat>
        <style>days</style>
        <from><get name="timeset3"/></from>
        <to><get name="timeget3"/></to>
        </interval></set>
    <set name="time4">
        <interval><jformat>MMMMMMMMM dd</jformat>
        <style>days</style>
        <from><get name="timeset4"/></from>
        <to><get name="timeget4"/></to>
        </interval></set>
    </think>
<condition name="ANSWER">
    <li value="ok">
        <condition name="time1">
            <li value="0">
                <condition name="time3">
                    <li value="0">
                        <condition name="subnumber1">
                            <li value="2">
                                <condition name="subnumber3">
                                    <li value="2">We talked about <get name="subject1"/> and <get name="subject3"/><think><set name="ANSWER">false</set></think></li>
                                </condition>
                            </li>    
                        </condition>
                    </li>
                </condition>
            </li>
        </condition>
    </li>
</condition>
<condition name="ANSWER">
    <li value="ok">
        <condition name="time1">
            <li value="0">
                <condition name="time2">
                    <li value="0">
                        <condition name="subnumber1">
                            <li value="2">
                                <condition name="subnumber2">
                                    <li value="2">We talked about <get name="subject1"/> and <get name="subject2"/><think><set name="ANSWER">false</set></think></li>
                                </condition>
                            </li>    
                        </condition>
                    </li>
                </condition>
            </li>
        </condition>
    </li>
</condition>
<condition name="ANSWER">
    <li value="ok">
        <condition name="time1">
            <li value="0">
                <condition name="time2">
                    <li value="0">
                        <condition name="subnumber2">
                            <li value="2">We talked about <get name="subject2"/><think><set name="ANSWER">false</set></think></li>
                        </condition>
                    </li>    
                </condition>
            </li>
        </condition>
    </li>
</condition>
<condition name="ANSWER">
    <li value="ok">
        <condition name="time1">
            <li value="0">
                <condition name="subnumber1">
                    <li value="2">We talked about <get name="subject1"/><think><set name="ANSWER">false</set></think></li>
                </condition>
            </li>    
        </condition>
    </li>
</condition>
</template></category></learn></think>
<oob><mrl><service>python</service><method>exec</method><param>
GETMEMO()
</param></mrl></oob>
</template></category>

</aiml>
